CC = gcc
SRC = school.c main.c
OBJ = $(SRC:.c=.o)
NAME = school
RM = rm -f 
CFLAGS = -Wall -Werror -Wextra -pedantic
all:$(NAME)
%.o:%.c
	$(CC) $(CFLAGS) -c -o $@ $<
$(NAME):$(OBJ)
	$(CC) $(CFLAGS) -o $@ $^

clean:
	$(RM) *~ $(NAME)
oclean:
	$(RM) $(OBJ)
fclean: clean oclean
re: fclean all
.PHONY: all clean oclean fclean re
